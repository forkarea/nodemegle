{
  "name": "omagleclone",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start_server": "nodemon ./bin/www --exec babel-node --presets es2015,stage-1",
    "start_webpack": "node ./node_modules/webpack/bin/webpack.js --colors --watch --display-error-details",
    "start_webpackserver": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "start": "npm-run-all --parallel start_server start_webpack"
  },
  "dependencies": {
    "babel-preset-stage-1": "^6.16.0",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "lodash": "^4.16.6",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "css-loader": "^0.25.0",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.9.0",
    "gulp-sass": "^2.3.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.1",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
